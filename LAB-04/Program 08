#include <iostream>
using namespace std;

int main() {
	int a, b, c;
	cout<<"Enter the 1st num : ";
	cin>>a;
	cout<<"Enter the 2nd num : ";
	cin>>b;
	cout<<"Enter the 3rd num : ";
	cin>>c;
	
	if(a<b && a<c)
	cout<<a<<" is the smallest.";
	
	else if(b<a && b<c)
	cout<<b<<" is the smallest.";
	
	else
	cout<<c<<" is the smallest.";
	
	return 0;
}
