#include <iostream>
using namespace std;

int main() {
 float marks;
 cout<<"Enter your marks : ";
 cin>>marks;
 if(marks>=90)
 cout<<"You got A grade.";
 else if(marks>=80 && marks<90)
 cout<<"You got B grade.";
 else if(marks>=70 && marks<80)
 cout<<"You got C grade.";
 else if(marks>=60 && marks<70)
 cout<<"You got D grade.";
 else if(marks<60)
 cout<<"You got F grade.";
 else 
 cout<<"Invalid Input";
 
return 0;
}
