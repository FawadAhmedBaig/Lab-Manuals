#include <iostream>
using namespace std;
	int main() {
    int n,a,c1=0,c2=0,c3=0;
	cout<<"Enter the inputs you want : ";
	cin>>n;
    for(int i = 1 ; i<=n ; i++)
    {
    	cin>>a;
    	if(a>0)
    	{
    		c1++;
		}
		else if(a==0)
    	{
    		c2++;
		}
		else
		c3++;
	}
    cout<<"The number of positive inputs : "<<c1<<endl;
    cout<<"The number of negative inputs : "<<c3<<endl;
    cout<<"The number of zeros entered   : "<<c2<<endl;

	return 0;
}
