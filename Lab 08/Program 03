#include <iostream>
using namespace std;
int main(int argc, char** argv) {
int val , s , loc = -1;
cout<<"Enter the size of array : ";
cin>>s;
int arr[s];
for(int i = 0 ; i<s; i++)
{
cout<<"Enter the vlaue at index "<<i<<" : ";
cin>>arr[i];
}
cout<<"Enter the value to search : ";
cin>>val;

for(int i = 0 ; i < s; i++)
{
if(val==arr[i])
{
loc = i;
break;
}
}
if(loc==-1)
cout<<"not found";
else
cout<<"Number found";

	return 0;
}
