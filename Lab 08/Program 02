#include <iostream>
using namespace std;
int main() {
	int arr[5];
	int val, loc = -1;
    for(int i = 0 ; i<5 ; i++)
    {
    	cout<<"Enter the value at index "<<i<<" : ";
    	cin>>arr[i];
	}
	cout<<"Enter the value to search : ";
	cin>>val;
	for(int i = 0 ; i<5 ; i++)
	{
		if(val==arr[i])
		loc = i;
	}
    if(loc==-1)
    cout<<"Number Not Found.";
    else
    cout<<"Number Found.";

   return 0;
}
