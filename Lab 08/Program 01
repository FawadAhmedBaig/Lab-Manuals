#include <iostream>
using namespace std;
int main(int argc, char** argv) {
int val , loc = -1;
int arr[5]={12,123,45,23,99};
for(int i = 0 ; i < 5; i++)
{
cout<<arr[i]<<" ";
}
cout<<"\nEnter the value to search : ";
cin>>val;

for(int i = 0 ; i < 5; i++)
{
if(val==arr[i])
{
loc = i;
break;
}
}
if(loc==-1)
cout<<"not found";
else
cout<<val<<" found at index : "<<loc;

	return 0;
}
